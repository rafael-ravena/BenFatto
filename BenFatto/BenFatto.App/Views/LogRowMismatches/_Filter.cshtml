@model BenFatto.App.DTO.LogRowMismatch
<form asp-action="Index" method="get">
    <div class="row">
        <div class="col-md-4">
            <div class="row">
                <label asp-for="ThrownException" class="col-form-label col-md-3 text-right"></label>
                <div class="col"><input asp-for="ThrownException" class="form-control" /></div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="row">
                <label asp-for="Row" class="col-form-label col-md-3 text-right"></label>
                <div class="col"><input asp-for="Row" class="form-control" /></div>
            </div>
        </div>
        <div class="col text-right">
            <input type="hidden" name="page" id="page" value="@(Context.Request.Query["page"])" />
            <input type="submit" value="Search" class="btn btn-primary" />
        </div>
    </div>
</form>
